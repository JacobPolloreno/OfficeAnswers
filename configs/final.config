{
   "net_name":"sample",
   "paths":{
      "data_dir":"./data/",
      "preprocess_dir":"./data/preprocessed",
      "processed_dir":"./data/processed",
      "log_dir":"./logs/"
   },
   "inputs":{
      "share":{
         "raw_corpus":"./data/raw/raw_wiki_data.txt",
         "custom_corpus":"./data/raw/custom_IT.tsv",
         "train_test_split":"0.8,0.1,0.1"
      },
      "train":{
         "phase":"TRAIN",
         "batch_size":100,
         "steps_per_epoch":200,
         "epochs":20,
         "verbose":1
      },
      "valid":{
         "phase":"EVAL",
         "batch_list":10
      },
      "test":{
         "phase":"EVAL",
         "batch_list":10
      },
      "predict":{
         "phase":"PREDICT"
      }
   },
   "outputs":{
      "predict":{
         "save_path":"predict.txt"
      }
   },
   "model":{
      "type":"DSSM",
      "setting":{
         "hidden_sizes":[
            300
         ],
         "dropout_rate":0.9
      }
   }
}
