{
   "paths":{
      "data_dir":"./data/",
      "preprocess_dir":"./data/preprocessed",
      "processed_dir":"./data/processed",
      "weights_file":"../data/processed/backend.h5",
      "params_file":"../data/processed/params.dill",
      "log_dir":"./logs/"
   },
   "inputs":{
      "share":{
         "raw_corpus":"./data/raw/raw_wiki_data.txt",
	 "custom_corpus": "",
         "documents":"documents.dill",
         "questions":"questions.dill",
	 "train_test_split":"0.8,0.1,0.1"
      },
      "train":{
         "phase":"TRAIN",
         "batch_size":100,
         "steps_per_epoch":200,
         "epochs":3,
	 "verbose":1,
         "datapack":"train.dill"
      },
      "valid":{
         "phase":"EVAL",
         "batch_list":10,
         "datapack":"valid.dill"
      },
      "test":{
         "phase":"EVAL",
         "batch_list":10,
         "datapack":"test.dill"
      },
      "predict":{
         "phase":"PREDICT"
      }
   },
   "outputs":{
      "predict":{
         "save_path":"predict.txt"
      }
   },
   "model":{
      "type":"DSSM",
      "setting":{
         "hidden_sizes":[
            300
         ],
         "dropout_rate":0.9
      }
   }
}
